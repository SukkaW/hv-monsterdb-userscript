{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2020", "dom", "dom.iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "million",
    "module": "preserve",
    "moduleResolution": "bundler",
    "types": ["greasemonkey", "greasemonkey3"],
    "resolveJsonModule": true,
    "declaration": false,
    "outDir": ".",
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["src"],
  "exclude": ["dist", "test"]
}
