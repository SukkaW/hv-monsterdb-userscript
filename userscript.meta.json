{
  "name": "HentaiVerse Monster Database UserScript",
  "namespace": "https://hentaiverse.org",
  "grant": [
    "unsafeWindow",
    "GM.getValue",
    "GM.setValue",
    "GM.deleteValue"
  ],
  "run-at": "document-idle",
  "compatible": [
    "Chrome/Chromium + Tampermonkey",
    "Firefox + Greasemonkey"
  ],
  "match": [
    "*://*.hentaiverse.org/*"
  ],
  "exclude": [
    "http*://hentaiverse.org/pages/showequip.php?*",
    "*hentaiverse.org/equip/*"
  ]
}
