  "homepage": "https://github.com/SukkaW/hv-monsterdb-userscript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SukkaW/hv-monsterdb-userscript.git"
  },
  "bugs": {
    "url": "https://github.com/SukkaW/hv-monsterdb-userscript/issues"
  },
  "main": "dist/hv-monsterdb.es2020.user.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup --configPlugin rollup-plugin-swc3 -c --bundleConfigAsCjs",
    "build:analyze": "ANALYZE=true pnpm run build",
    "doc": "typedoc",
    "lint": "eslint --format=sukka .",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "HentaiVerse"
  ],
  "author": "Sukka <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "foxact": "^0.2.49",
    "foxts": "^5.0.0",
    "million": "^1.12.5"
  },
  "devDependencies": {
    "@eslint-sukka/node": "^8.0.1",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@types/greasemonkey": "^4.0.7",
    "@types/greasemonkey3": "npm:@types/greasemonkey@^3.0.3",
    "@types/node": "^22.18.12",
    "core-js": "^3.46.0",
    "cssnano": "^7.1.1",
    "eslint": "^9.38.0",
    "eslint-config-sukka": "^8.0.1",
    "eslint-formatter-sukka": "^8.0.1",
    "magic-string": "^0.30.19",
    "nanostores": "^1.0.1",
    "rollup": "^4.52.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc3": "^0.12.1",
    "rollup-plugin-userscript-metablock": "^0.4.3",
    "typed-query-selector": "^2.12.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vite-bundle-analyzer": "^1.2.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "pnpm": {
    "overrides": {
      "cssnano": "$cssnano",
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "eslint>chalk": "npm:picocolors@^1.1.1",
      "has": "npm:@nolyfill/has@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest"
    }
  }
